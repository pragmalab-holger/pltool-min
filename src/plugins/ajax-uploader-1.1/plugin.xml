<plugin name='ajax-uploader' version='1.1' grailsVersion='2.2.5 &gt; *'>
  <author>Antony Jones</author>
  <authorEmail>aj@desirableobjects.co.uk</authorEmail>
  <title>Ajax Uploader Plugin</title>
  <description>Highly configurable cross-browser, ajax-based file uploader.
    Displays a progress bar whilst uploading, allows restriction by filetype, and multiple file upload.

    Based on Valums 'ajax uploader' javascript.
    Testtext bla
  </description>
  <documentation>http://grails.org/plugin/ajax-uploader</documentation>
  <type>AjaxUploaderGrailsPlugin</type>
  <resources>
    <resource>AjaxUploaderResources</resource>
    <resource>uk.co.desirableobjects.ajaxuploader.AjaxUploadController</resource>
    <resource>uk.co.desirableobjects.ajaxuploader.AjaxUploaderService</resource>
    <resource>uk.co.desirableobjects.ajaxuploader.AjaxUploaderTagLib</resource>
    <resource>uk.co.desirableobjects.ajaxuploader.exception.FileUploadException</resource>
    <resource>uk.co.desirableobjects.ajaxuploader.exception.InvalidAttributeValueException</resource>
    <resource>uk.co.desirableobjects.ajaxuploader.exception.MissingRequiredAttributeException</resource>
    <resource>uk.co.desirableobjects.ajaxuploader.exception.UnknownAttributeException</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://repo.desirableobjects.co.uk' url='http://repo.desirableobjects.co.uk/' />
  </repositories>
  <dependencies />
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='jasper' version='1.7.0' />
      <plugin group='org.grails.plugins' name='quartz' version='1.0.1' />
      <plugin group='org.grails.plugins' name='cache' version='1.0.1' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='standalone' version='1.2.3' />
      <plugin group='org.grails.plugins' name='spring-security-acl' version='1.1.1' />
      <plugin group='org.grails.plugins' name='database-migration' version='1.3.2' />
      <plugin group='org.grails.plugins' name='twitter-bootstrap' version='2.3.0' />
      <plugin group='org.grails.plugins' name='hibernate' version='2.2.5' />
      <plugin group='org.grails.plugins' name='spring-security-ui' version='0.2' />
      <plugin group='org.grails.plugins' name='spring-security-core' version='1.2.7.3' />
      <plugin group='org.grails.plugins' name='lesscss-resources' version='1.3.3' />
      <plugin group='org.grails.plugins' name='resources' version='1.1.6' />
      <plugin group='org.grails.plugins' name='mysql-connectorj' version='5.1.22.1' />
      <plugin group='org.grails.plugins' name='famfamfam' version='1.0.1' />
    </runtime>
    <build>
      <plugin group='org.grails.plugins' name='tomcat' version='2.2.5' />
    </build>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>